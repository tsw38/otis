{
  "name": "@tsw38/otis",
  "version": "0.0.6",
  "description": "An opinionated React testing package",
  "bin": {
    "otis": "bin/cli.js"
  },
  "main": "./lib/index.js",
  "scripts": {
    "dev": "WATCH=true node parcel.bundler.js",
    "build": "node parcel.bundler.js",
    "prepublish": "npm run build",
    "publish": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsw38/otis.git"
  },
  "author": "Tyler Williams",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/tsw38/otis/issues"
  },
  "engines": {
    "node": ">=12.13.0",
    "npm": ">=6.12.0"
  },
  "homepage": "https://github.com/tsw38/otis#readme",
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-react": "7.12.10",
    "clone-deep": "4.0.1",
    "deepmerge": "4.2.2",
    "eslint": "7.15.0",
    "eslint-plugin-jest": "24.1.3",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-shebang": "1.3.1",
    "prettier": "2.2.1"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "7.12.11",
    "@babel/preset-env": "7.12.11",
    "@jackfranklin/test-data-bot": "1.3.0",
    "@react-mock/localstorage": "0.1.2",
    "@testing-library/cypress": "7.0.2",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@testing-library/user-event": "12.6.0",
    "@tsw38/custom-logger": "2.0.0",
    "chalk": "4.1.0",
    "command-line-usage": "6.1.1",
    "cypress": "6.1.0",
    "identity-obj-proxy": "3.0.0",
    "is-cli": "2.0.0",
    "jest": "26.6.0",
    "jest-axe": "4.1.0",
    "jest-chain": "1.1.5",
    "jest-date-mock": "1.0.8",
    "jest-extended": "0.11.5",
    "jest-svg-transformer": "1.0.0",
    "jest-watch-typeahead": "0.6.1",
    "minimist": "1.2.5",
    "msw": "0.24.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-react-jsx"
    ]
  },
  "shebang": [
    {
      "interpreter": "node",
      "files": [
        "./bin/cli.js"
      ]
    }
  ]
}
