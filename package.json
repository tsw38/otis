{
  "name": "@tsw38/otis",
  "version": "1.0.10-beta.6",
  "description": "An opinionated React testing package",
  "bin": {
    "otis": "src/cli/index.js"
  },
  "main": "./index.js",
  "scripts": {
    "dev": "WATCH=true node parcel.bundler.js",
    "build": "npm run build:m && npm run build:b",
    "build:b": "rollup --config rollup/bin.config.js",
    "build:m": "rollup --config rollup/modules.config.js",
    "prerelease": "npm run build",
    "release": "npm publish --access public",
    "changelog": "auto-changelog -p && git add CHANGELOG.md",
    "pre:major": "npm run changelog",
    "major": "release major",
    "pre:minor": "npm run changelog",
    "minor": "release minor",
    "pre:patch": "npm run changelog",
    "patch": "release patch",
    "beta": "release pre beta"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsw38/otis.git"
  },
  "author": "Tyler Williams",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/tsw38/otis/issues"
  },
  "engines": {
    "node": ">=12.13.0",
    "npm": ">=6.12.0"
  },
  "homepage": "https://github.com/tsw38/otis#readme",
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "clone-deep": "4.0.1",
    "deepmerge": "4.2.2",
    "eslint": "7.15.0",
    "eslint-plugin-jest": "24.1.3",
    "husky": "4.3.6",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-shebang": "1.3.1",
    "prettier": "2.2.1",
    "start-server-and-test": "1.11.6"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-transform-react-jsx": "7.12.11",
    "@babel/polyfill": "7.12.1",
    "@jackfranklin/test-data-bot": "1.3.0",
    "@react-mock/localstorage": "0.1.2",
    "@rollup/plugin-alias": "3.1.2",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "11.2.0",
    "@testing-library/cypress": "7.0.2",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@testing-library/user-event": "12.6.0",
    "@tsw38/custom-logger": "2.0.1",
    "auto-changelog": "2.2.1",
    "chalk": "4.1.0",
    "command-line-usage": "6.1.1",
    "concurrently": "5.3.0",
    "core-js": "3.9.1",
    "cypress": "6.2.0",
    "glob": "7.1.6",
    "identity-obj-proxy": "3.0.0",
    "is-cli": "2.0.0",
    "jest": "26.6.0",
    "jest-axe": "4.1.0",
    "jest-chain": "1.1.5",
    "jest-date-mock": "1.0.8",
    "jest-extended": "0.11.5",
    "jest-svg-transformer": "1.0.0",
    "jest-watch-typeahead": "0.6.1",
    "minimist": "1.2.5",
    "mkdirp": "1.0.4",
    "msw": "0.24.2",
    "release": "6.3.0",
    "rollup": "2.41.4",
    "rollup-plugin-executable": "1.6.2",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-preserve-shebang": "1.0.1",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-visualizer": "4.2.1"
  },
  "peerDependencies": {
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": 3
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-react-jsx"
    ]
  },
  "shebang": [
    {
      "interpreter": "node",
      "files": [
        "./bin/cli.js"
      ]
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && git add ."
    }
  }
}
