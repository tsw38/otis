{
  "name": "usage",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cra-template": "1.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:server": "npm run build && serve -s build",
    "test": "otis test",
    "test:watch": "otis test -w",
    "test:e2e": "otis test -e --startCommand=test:server",
    "test:unit": "otis test -u",
    "test:e2e:watch": "npm run test:e2e -- -w",
    "test:unit:watch": "otis test -u -w",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "@tsw38/otis/lib/modules/preset-env",
      "@tsw38/otis/lib/modules/preset-react"
    ],
    "plugins": [
      "@tsw38/otis/lib/modules/plugin-transform-react-jsx"
    ]
  },
  "jest": {
    "reporters": [
      [
        "@tsw38/otis/lib/modules/threshold-ratchet",
        {
          "tolerance": 10,
          "roundDown": true
        }
      ]
    ],
    "coverageThreshold": {
      "global": {
        "lines": 0,
        "statements": 0,
        "functions": 23,
        "branches": 0
      }
    }
  },
  "devDependencies": {
    "serve": "^11.3.2"
  }
}
