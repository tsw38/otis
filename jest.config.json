{
    "moduleNameMapper": {
      "\\.(svg|css)$": "@tsw38/otis/lib/map-to-string"
    },
    "setupFilesAfterEnv": [
      "@tsw38/otis/lib/extend-expect"
    ],
    "reporters": [
      "default"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary",
      "json-summary"
    ],
    "watchPlugins": [
      "@tsw38/otis/lib/watch-typeahead-filename",
      "@tsw38/otis/lib/watch-typeahead-testname"
    ]
  }